@model GioiThieuSanPham.Models.ACCOUNT

@{
    ViewBag.Title = "Register";
}


<h2>Register</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
<div class="form-horizontal" style="background-color:bisque;font-size: 26px;">
    <hr>
    <div class="form-group">
        <label class="control-label col-md-2" for="EMAIL">EMAIL</label>
        <div class="col-sm-6">
            <input class="form-control text-box single-line" style="min-width: 900px;height: 40px;" id="EMAIL" name="EMAIL" type="text" value="">
            <span class="field-validation-valid text-danger" data-valmsg-for="EMAIL" data-valmsg-replace="true"></span>
        </div>
    </div>



    <div class="form-group">
        <label class="control-label col-md-2" for="MATKHAU">MATKHAU</label>
        <div class="col-sm-6">
            <input class="form-control text-box single-line" style=" min-width: 900px;height: 40px;" id="MATKHAU" name="MATKHAU" type="text" value="">
            <span class="field-validation-valid text-danger" data-valmsg-for="MATKHAU" data-valmsg-replace="true"></span>
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-2" for="TRANGTHAI">TRANGTHAI</label>
        <div class="col-sm-6">
            <div class="checkbox">
                <input class="check-box" data-val="true" style="min-width: 0px;height: 40px;" data-val-required="The Status field is required." id="TRANGTHAI" name="TRANGTHAI" type="checkbox" value="true"><input name="TRANGTHAI" type="hidden" value="false">
                <span class="field-validation-valid text-danger" data-valmsg-for="TRANGTHAI" data-valmsg-replace="true"></span>
            </div>
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-2" for="CHUADMIN">CHUADMIN</label>
        <div class="col-sm-6">
            <div class="checkbox">
                <input class="check-box" data-val="true" style=" min-width: 0px;height: 40px;" data-val-required="The IsAdmin field is required." id="CHUADMIN" name="CHUADMIN" type="checkbox" value="true"><input name="CHUADMIN" type="hidden" value="false">
                <span class="field-validation-valid text-danger" data-valmsg-for="CHUADMIN" data-valmsg-replace="true"></span>
            </div>
        </div>
    </div>





    <div class="form-group">
        <div class="col-md-offset-2 col-sm-6">
            <a href=""> <input type="submit" value="Register" style="background-color: #f7d09e" class="btn btn-default"></a>
        </div>
    </div>
</div>

}
<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
